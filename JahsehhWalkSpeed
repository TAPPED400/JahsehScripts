-- Services
local player = game.Players.LocalPlayer
local runService = game:GetService("RunService")
local lighting = game:GetService("Lighting")

-- Blur effect
local blur = Instance.new("BlurEffect")
blur.Size = 56
blur.Parent = lighting

-- -- Create ScreenGui for the loading screen
local loadingGui = Instance.new("ScreenGui")
loadingGui.Name = "LoadingGui"
loadingGui.ResetOnSpawn = false
loadingGui.Parent = game.CoreGui

-- Loading Screen Frame inside the GUI
local loading = Instance.new("Frame")
loading.Size = UDim2.new(1, 0, 1, 0)
loading.AnchorPoint = Vector2.new(0.5, 0.5)
loading.Position = UDim2.new(0.5, 0, 0.5, 0)
loading.BackgroundColor3 = Color3.new(0, 0, 0)
loading.BackgroundTransparency = 0.4
loading.ZIndex = 1000
loading.Parent = loadingGui

-- Centered Discord Link Label
local discordText = Instance.new("TextLabel")
discordText.Text = "Join discord.gg/d28AHxfKqD"
discordText.Size = UDim2.new(0, 400, 0, 50)
discordText.Position = UDim2.new(0.5, -200, 0.5, -25)
discordText.BackgroundTransparency = 1
discordText.TextColor3 = Color3.new(1, 1, 1)
discordText.Font = Enum.Font.SourceSansBold
discordText.TextSize = 28
discordText.ZIndex = 1001
discordText.Parent = loading

-- Rainbow animation for Discord text
runService.RenderStepped:Connect(function()
pcall(function()
if discordText and discordText.Parent then
discordText.TextColor3 = Color3.fromHSV(tick() % 5 / 5, 1, 1)
end
end)
end)

-- Delay GUI appearance until loading ends
task.delay(3, function()
for i = 0, 1, 0.05 do
loading.BackgroundTransparency = 0.4 + (i * 0.6)
discordText.TextTransparency = i
task.wait(0.02)
end
loading:Destroy()
blur:Destroy()

-- After fade-out, create the GUI  
local gui = Instance.new("ScreenGui", game.CoreGui)  
gui.Name = "WalkSpeedByjahseh"  
gui.ResetOnSpawn = false  

local walkSpeed = 500  
local jumpPower = 120  
local walkEnabled = true  
local jumpEnabled = false  

local function rainbowText(obj)  
	runService.RenderStepped:Connect(function()  
		pcall(function()  
			obj.TextColor3 = Color3.fromHSV(tick() % 5 / 5, 1, 1)  
		end)  
	end)  
end  

local function createBtn(name, pos, text)  
	local frame = Instance.new("Frame", gui)  
	frame.Name = name  
	frame.Size = UDim2.new(0, 140, 0, 45)  
	frame.Position = pos  
	frame.BackgroundColor3 = Color3.new(0, 0, 0)  
	frame.Active = true  
	frame.Draggable = true  
	Instance.new("UICorner", frame)  

	local btn = Instance.new("TextButton", frame)  
	btn.Size = UDim2.new(1, -10, 1, -10)  
	btn.Position = UDim2.new(0, 5, 0, 5)  
	btn.Text = text  
	btn.Font = Enum.Font.SourceSansBold  
	btn.TextSize = 16  
	btn.BackgroundColor3 = Color3.new(0, 0, 0)  
	btn.TextColor3 = Color3.new(1, 1, 1)  
	rainbowText(btn)  
	return btn  
end  

local speedBtn = createBtn("SpeedFrame", UDim2.new(0, 50, 0, 100), "Speed: ON")  
local jumpBtn = createBtn("JumpFrame", UDim2.new(0, 50, 0, 160), "Jump: OFF")  

local customFrame = Instance.new("Frame", gui)  
customFrame.Size = UDim2.new(0, 250, 0, 180)  
customFrame.Position = UDim2.new(0, 300, 0, 100)  
customFrame.BackgroundColor3 = Color3.new(0, 0, 0)  
customFrame.Active = true  
customFrame.Draggable = true  
Instance.new("UICorner", customFrame)  

local function createInput(placeholder, y)  
	local input = Instance.new("TextBox", customFrame)  
	input.PlaceholderText = placeholder  
	input.Size = UDim2.new(0, 200, 0, 30)  
	input.Position = UDim2.new(0, 25, 0, y)  
	input.BackgroundColor3 = Color3.fromRGB(30, 30, 30)  
	input.TextColor3 = Color3.new(1, 1, 1)  
	input.Font = Enum.Font.SourceSans  
	input.TextSize = 16  
	rainbowText(input)  
	return input  
end  

local wsInput = createInput("WalkSpeed (e.g. 500)", 30)  
local jpInput = createInput("JumpPower (e.g. 120)", 70)  

local applyBtn = Instance.new("TextButton", customFrame)  
applyBtn.Size = UDim2.new(0, 200, 0, 30)  
applyBtn.Position = UDim2.new(0, 25, 0, 120)  
applyBtn.Text = "Apply Settings"  
applyBtn.Font = Enum.Font.SourceSansBold  
applyBtn.TextSize = 16  
applyBtn.BackgroundColor3 = Color3.fromRGB(40, 180, 40)  
applyBtn.TextColor3 = Color3.new(1, 1, 1)  
rainbowText(applyBtn)  

local toggleBtn = Instance.new("TextButton", gui)  
toggleBtn.Size = UDim2.new(0, 130, 0, 30)  
toggleBtn.Position = UDim2.new(0, 300, 0, 70)  
toggleBtn.Text = "Open Customizer"  
toggleBtn.BackgroundColor3 = Color3.new(0, 0, 0)  
toggleBtn.Font = Enum.Font.SourceSansBold  
toggleBtn.TextSize = 16  
toggleBtn.TextColor3 = Color3.new(1, 1, 1)  
toggleBtn.Active = true  
toggleBtn.Draggable = true  
rainbowText(toggleBtn)  

local watermark = Instance.new("TextLabel", gui)  
watermark.Size = UDim2.new(1, 0, 0, 25)  
watermark.Position = UDim2.new(0, 0, 1, -25)  
watermark.BackgroundTransparency = 1  
watermark.Text = "jahsehhh"  
watermark.Font = Enum.Font.SourceSansBold  
watermark.TextSize = 20  
watermark.TextStrokeTransparency = 0.5  
watermark.TextColor3 = Color3.new(1, 1, 1)  
rainbowText(watermark)  

speedBtn.MouseButton1Click:Connect(function()  
	walkEnabled = not walkEnabled  
	speedBtn.Text = walkEnabled and "Speed: ON" or "Speed: OFF"  
end)  

jumpBtn.MouseButton1Click:Connect(function()  
	jumpEnabled = not jumpEnabled  
	jumpBtn.Text = jumpEnabled and "Jump: ON" or "Jump: OFF"  
end)  

applyBtn.MouseButton1Click:Connect(function()  
	local ws = tonumber(wsInput.Text)  
	local jp = tonumber(jpInput.Text)  
	if ws then walkSpeed = ws end  
	if jp then jumpPower = jp end  

	local hum = player.Character and player.Character:FindFirstChildWhichIsA("Humanoid")  
	if hum then  
		hum.WalkSpeed = walkEnabled and walkSpeed or 16  
		hum.JumpPower = jumpEnabled and jumpPower or 50  
	end  
end)  

toggleBtn.MouseButton1Click:Connect(function()  
	customFrame.Visible = not customFrame.Visible  
	toggleBtn.Text = customFrame.Visible and "Close Customizer" or "Open Customizer"  
end)  

runService.RenderStepped:Connect(function()  
	local hum = player.Character and player.Character:FindFirstChildWhichIsA("Humanoid")  
	if hum then  
		hum.WalkSpeed = walkEnabled and walkSpeed or 16  
		hum.JumpPower = jumpEnabled and jumpPower or 50  
	end  
end)

end)

